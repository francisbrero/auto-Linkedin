{
  "manifest_version": 3,
  "name": "LinkedIn Automation",
  "version": "1.2.1",
  "description": "Automates handling of LinkedIn connection requests.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "icons": {
    "128": "icon.png"
  }
}
